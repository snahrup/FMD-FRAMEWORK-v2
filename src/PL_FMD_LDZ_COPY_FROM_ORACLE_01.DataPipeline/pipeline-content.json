                  "oracleReaderQuery": {
                    "value": "SELECT REPLACE(\n  'SELECT ' || RTRIM(XMLAGG(XMLELEMENT(e,\n    CASE\n      WHEN data_type IN ('NUMBER')\n      AND data_precision IS NOT NULL\n      AND data_scale > 16\n        THEN 'CAST(\"' || column_name || '\" AS NUMBER(' || data_precision || ',' || 16 || ')) AS \"' || column_name || '\"'\n      WHEN data_type IN ('NUMBER')\n      AND data_precision IS NOT NULL\n      and data_scale <= 16\n        THEN 'CAST(\"' || column_name || '\" AS NUMBER(' || data_precision || ',' || data_scale || ')) AS \"' || column_name || '\"'\n      WHEN data_type in ('NUMBER')\n        THEN 'CAST(\"' || column_name || '\" AS NUMBER(36,0)) AS \"' || column_name || '\"'\n      WHEN data_type IN ('DECIMAL', 'FLOAT')\n        THEN 'CAST(\"' || column_name || '\" AS DECIMAL(38,16)) AS \"' || column_name || '\"'\n      WHEN data_type IN ('SDO_GEOMETRY')\n        THEN '(SDO_UTIL.TO_GEOJSON(\"' || column_name || '\")) AS \"' || column_name || '\"'\n      WHEN data_type IN ('DATE')\n        THEN 'CAST(\"' || column_name || '\" AS TIMESTAMP) AS \"' || column_name || '\"'\n      ELSE '\"' || column_name || '\"'\n    END || ', ').EXTRACT('//text()') ORDER BY column_id).GETCLOBVAL(), ', ') || \n  ' FROM \"' || owner || '\".\"' || table_name || '\"', '&quot;', '\"') AS QUERY\nFROM all_tab_columns\nWHERE owner = UPPER('@item().SourceSchema}')\n  AND table_name = UPPER('@item().SourceName}')\nGROUP BY owner, table_name",
